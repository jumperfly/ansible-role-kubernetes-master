apiVersion: v1
clusters:
- cluster:
    certificate-authority: {{ kube_controller_manager_ca_cert }}
    server: https://127.0.0.1:6443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: system:kube-controller-manager
  name: default
current-context: "default"
kind: Config
preferences: {}
users:
- name: system:kube-controller-manager
  user:
    client-certificate: {{ kube_controller_manager_cert }}
    client-key: {{ kube_controller_manager_key }}
